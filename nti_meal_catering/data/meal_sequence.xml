<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Sequence for Meal Order -->
        <record id="seq_meal_order" model="ir.sequence">
            <field name="name">Meal Order</field>
            <field name="code">meal.order</field>
            <field name="prefix">MO/%(y)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>